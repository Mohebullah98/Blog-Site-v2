<%- include("partials/header"); -%>
<h1>Home</h1>
<p>  <%=startContent%>  </p>
<% posts.forEach(function(post){%>
<h2><%=post.title%></h2>
<!-- Only want to print first 100 characters of body for each post. -->
<p><%=post.body.slice(0,100)+" ..."%><a href="<%=`/posts/${post._id}` %>">Read More</a></p>

<%});%>
<%- include("partials/footer"); -%>
